scriptionHtml, github, external, techs } = project;\r\n\r\n            return (\r\n              <CSSTransition\r\n                key={title}\r\n                classNames=\"fadeup\"\r\n                timeout={i >= PROJECTS_GRID_LIMIT ? (i - PROJECTS_GRID_LIMIT) * 300 : 300}\r\n                exit={false}\r\n              >\r\n                <StyledProject\r\n                  key={title}\r\n                  ref={(el) => (revealProjects.current[i] = el)}\r\n                  tabIndex=\"0\"\r\n                  style={{\r\n                    transitionDelay: `${\r\n                      i >= PROJECTS_GRID_LIMIT ? (i - PROJECTS_GRID_LIMIT) * 100 : 0\r\n                    }ms`,\r\n                  }}\r\n                >\r\n                  <div className=\"project-inner\">\r\n                    <header>\r\n                      <div className=\"project-top\">\r\n                        <div className=\"folder\">\r\n                          <Icon name=\"Folder\" />\r\n                        </div>\r\n                        <div className=\"project-links\">\r\n                          {github && (\r\n                            <a onClick={() => handleClickProject(github)} aria-label=\"GitHub Link\">\r\n                              <Icon name=\"GitHub\" />\r\n                            </a>\r\n                          )}\r\n                          {external && (\r\n                            <a\r\n                              onClick={() => handleClickProject(external)}\r\n                              aria-label=\"External Link\"\r\n                            >\r\n                              <Icon name=\"External\" />\r\n                            </a>\r\n                          )}\r\n                        </div>\r\n                      </div>\r\n\r\n                      <h3 className=\"project-title\">{title}</h3>\r\n\r\n                      <div\r\n                        className=\"project-description\"\r\n                        dangerouslySetInnerHTML={{ __html: descriptionHtml }}\r\n                      />\r\n                    </header>\r\n\r\n                    <footer>\r\n                      {techs && (\r\n                        <ul className=\"project-tech-list\">\r\n                          {techs.map((tech) => (\r\n                            <li key={tech}>{tech}</li>\r\n                          ))}\r\n                        </ul>\r\n                      )}\r\n                    </footer>\r\n                  </div>\r\n                </StyledProject>\r\n              </CSSTransition>\r\n            );\r\n          })}\r\n      </TransitionGroup>\r\n      {projects && projects.length > 6 && (\r\n        <button type=\"button\" className=\"more-button\" onClick={() => setShowMore(!showMore)}>\r\n          Show {showMore ? 'Less' : 'More'}\r\n        </button>\r\n      )}\r\n    </StyledProjectsSection>\r\n  );\r\n};\r\n\r\nexport default Projects;\r\n","C:\\Users\\LENOVO X1 YOGA\\Documents\\Dev\\portfoliov2\\src\\components\\Projects\\styles.js",[],"C:\\Users\\LENOVO X1 YOGA\\Documents\\Dev\\portfoliov2\\src\\components\\Side\\index.js",[],"C:\\Users\\LENOVO X1 YOGA\\Documents\\Dev\\portfoliov2\\src\\components\\Side\\styles.js",[],"C:\\Users\\LENOVO X1 YOGA\\Documents\\Dev\\portfoliov2\\src\\components\\Social\\index.js",[],"C:\\Users\\LENOVO X1 YOGA\\Documents\\Dev\\portfoliov2\\src\\components\\Social\\styles.js",[],"C:\\Users\\LENOVO X1 YOGA\\Documents\\Dev\\portfoliov2\\src\\lib\\constants.js",[],"C:\\Users\\LENOVO X1 YOGA\\Documents\\Dev\\portfoliov2\\src\\lib\\gtag.js",[],{"ruleId":"184","severity":1,"message":"185","line":16,"column":15,"nodeType":"186","messageId":"187","endLine":26,"endColumn":17},{"ruleId":"184","severity":1,"message":"185","line":41,"column":21,"nodeType":"186","messageId":"187","endLine":41,"endColumn":74},{"ruleId":"184","severity":1,"message":"185","line":94,"column":25,"nodeType":"186","messageId":"187","endLine":94,"endColumn":78},"react/no-